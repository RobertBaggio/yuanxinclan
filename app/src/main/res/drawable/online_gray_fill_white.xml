<?xml version="1.0" encoding="utf-8"?><!--
    * Describe: 顶部一条实线  中间填充白色 方便布局
    * Author: xjc
    -->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp">
        <shape>
            <stroke
                android:width="0.5dp"
                android:color="#e7e6eb"/>
            <solid android:color="@color/white"/>
        </shape>
    </item>
</layer-list>
