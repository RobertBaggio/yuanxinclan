<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
    >
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/item_gank_layout"
        android:background="@color/white"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="104dp"
            android:weightSum="6"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true">
            <ImageView
                android:id="@+id/item_business_district_library_image"
                android:layout_width="100dp"
                android:layout_height="85dp"
                android:layout_marginTop="5dp"
                android:scaleType="centerCrop"/>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="5.7"
                android:layout_marginLeft="12dp"
                android:layout_marginTop="5dp"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                <TextView
                    android:id="@+id/item_gank_namete"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="10dp"
                    android:ellipsize="end"
                    android:maxLines="2"
                    android:text="NAME"
                    android:textColor="#323232"
                    android:textSize="15dp"/>




                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:gravity="center_vertical"
                    android:layout_height="18dp"
                    android:layout_marginTop="5dp"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/addresste"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/busnissred"
                        android:textSize="12dp"
                        android:text="address"/>
                    <TextView
                        android:id="@+id/leixingte"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/busnissred"
                        android:textSize="12dp"
                        android:layout_marginLeft="13dp"
                        android:text="class"/>
                    <TextView
                        android:id="@+id/shangxite"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/busnissred"
                        android:textSize="12dp"
                        android:layout_marginLeft="13dp"
                        android:text="businessxi"/>
                </LinearLayout>
                <TextView
                    android:id="@+id/fuwuzhonglei"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/busnissred"
                    android:textSize="12dp"
                    android:layout_marginTop="5dp"
                    android:text=""/>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:orientation="horizontal">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/busnissredgry"
                        android:textSize="12dp"
                        android:text="会员 "/>
                    <TextView
                        android:id="@+id/huiyuante"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/busnissredgry"
                        android:textSize="12dp"
                        android:text="num"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/busnissredgry"
                        android:textSize="12dp"
                        android:layout_marginLeft="3dp"
                        android:drawableLeft="@drawable/business_list_dot"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/busnissredgry"
                        android:textSize="12dp"
                        android:layout_marginLeft="5dp"
                        android:text="收藏 "/>
                    <TextView
                        android:id="@+id/shoucte"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/busnissredgry"
                        android:textSize="12dp"
                        android:text="num"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/busnissredgry"
                        android:textSize="12dp"
                        android:layout_marginLeft="3dp"
                        android:visibility="gone"
                        android:drawableLeft="@drawable/business_list_dot"/>
                    <TextView
                        android:id="@+id/redute"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/busnissredgry"
                        android:textSize="12dp"
                        android:layout_marginLeft="5dp"
                        android:visibility="gone"
                        android:text="热度"/>

                </LinearLayout>



            </LinearLayout>



        </LinearLayout>
    </RelativeLayout>


    <TextView
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginTop="5dp"
        android:background="@color/gray_white"
        android:layout_below="@id/item_gank_layout"/>


</RelativeLayout>

