            android:layout_height="wrap_content"
            android:layout_marginLeft="13dp"
            android:layout_marginRight="13dp"
            android:layout_marginTop="15dp"
            android:background="@drawable/border_grey"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:gravity="center_vertical"
                android:paddingLeft="15dp"
                android:paddingRight="15dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="企业名称"
                    android:textColor="@color/common_register_black"
                    android:textSize="15dp"/>

                <ImageView
                    android:layout_width="7dp"
                    android:layout_height="7dp"
                    android:layout_marginLeft="1dp"
                    android:layout_marginTop="3dp"
                    android:src="@drawable/bi_tian"/>

                <TextView
                    android:layout_width="1dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="10dp"
                    android:background="@drawable/line"/>

                <EditText
                    android:id="@+id/activity_login_epnameet"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@null"
                    android:hint="请输入公司名称"
                    android:maxLength="16"
                    android:textColor="@color/common_register_black"
                    android:textSize="15dp"/>
            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@drawable/line"/>

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:gravity="center_vertical"
                android:paddingLeft="15dp"
                android:paddingRight="15dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="企业法人"
                    android:textColor="@color/common_register_black"
                    android:textSize="15dp"/>

                <ImageView
                    android:layout_width="7dp"
                    android:layout_height="7dp"
                    android:layout_marginLeft="1dp"
                    android:layout_marginTop="3dp"
                    android:src="@drawable/bi_tian"/>

                <TextView
                    android:layout_width="1dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="10dp"
                    android:background="@drawable/line"/>

                <EditText
                    android:id="@+id/activity_login_epmenet"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@null"
                    android:hint="请输入企业法人"
                    android:maxLength="16"
                    android:textColor="@color/common_register_black"
                    android:textSize="15dp"/>
            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@drawable/line"/>

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:gravity="center_vertical"
                android:paddingLeft="15dp"
                android:paddingRight="15dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="行业分类"
                    android:textColor="@color/common_register_black"
                    android:textSize="15dp"/>

                <ImageView
                    android:layout_width="7dp"
                    android:layout_height="7dp"
                    android:layout_marginLeft="1dp"
                    android:layout_marginTop="3dp"
                    android:src="@drawable/bi_tian"/>

                <TextView
                    android:layout_width="1dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="10dp"
                    android:background="@drawable/line"/>

                <TextView
                    android:id="@+id/actimember_epbsinesste"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@null"
                    android:drawableRight="@drawable/arrow_down"
                    android:hint="请选择行业分类"
                    android:maxLength="16"
                    android:textColor="@color/common_register_black"
                    android:textSize="15dp"/>
            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@drawable/line"/>

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:gravity="center_vertical"
                android:paddingLeft="15dp"
                android:paddingRight="15dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="统一信用代码"
                    android:textColor="@color/common_register_black"
                    android:textSize="15dp"/>

                <ImageView
                    android:layout_width="7dp"
                    android:layout_height="7dp"
                    android:layout_marginLeft="1dp"
                    android:layout_marginTop="3dp"
                    android:src="@drawable/bi_tian"/>

                <EditText
                    android:id="@+id/activity_logister_maet"
                    android:layout_width="0dp"
                    android:digits="0123456789abcdefghigklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@null"
                    android:hint="营业执照统一社会信用代码"
                    android:maxLength="18"
                    android:textColor="@color/common_register_black"
                    android:textSize="15dp"/>

                <TextView
                    android:layout_width="1dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="3dp"
                    android:layout_marginRight="6dp"
                    android:background="@drawable/line"/>

            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@drawable/line"/>
        </LinearLayout>


        <LinearLayout
