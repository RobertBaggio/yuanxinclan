                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:text="身份证背面"
                        android:textSize="13dp"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/defenoseleli"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:gravity="center_vertical|end"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/buyserverte"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="8dp"
                android:text="购买365元/年专人对接《服务》"
                android:textColor="@color/businesstop"
                android:textSize="14dp"/>

            <ImageView
                android:id="@+id/ifbugimage"
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:layout_marginRight="13dp"
                android:src="@drawable/eq_edit_choice_pre"/>

        </LinearLayout>

        <Button
            android:id="@+id/commit_register"
            android:layout_width="match_parent"
            android:layout_height="46dp"
            android:layout_marginLeft="13dp"
            android:layout_marginRight="13dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/selector_register_green"
            android:text="提交申请"
            android:textColor="@color/white"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginTop="20dp"
