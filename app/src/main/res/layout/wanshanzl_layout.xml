<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:background="@color/commom_register_grey"
              android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="43dp"
        android:orientation="horizontal"
        android:background="@color/businesstop"
        android:weightSum="5"
        android:gravity="center_vertical">
        <LinearLayout
            android:id="@+id/window_head_left_layout"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:paddingLeft="1dp"
            android:gravity="center_vertical">

            <ImageView
                android:id="@+id/window_head_left_image"
                android:layout_width="22dp"
                android:layout_height="22dp"
                android:layout_marginLeft="6dp"
                android:layout_gravity="center_vertical"
                android:scaleType="centerInside"
                android:src="@drawable/ease_mm_title_back"/>
        </LinearLayout>

        <TextView
            android:id="@+id/textView17"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="3.2"
            android:gravity="center"
            android:text="完善资料"
            android:textColor="@color/white"
            android:textSize="15.5dp"/>

        <TextView
            android:id="@+id/activity_login_common_register_login"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:text="登录"
            android:gravity="center"
            android:textSize="15dp"
            android:visibility="gone"
            android:textColor="@color/white"
            android:layout_weight="0.8"/>

    </LinearLayout>


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
